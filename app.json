{
  "expo": {
    "name": "ido-mobile",
    "slug": "ido-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/src/assets/logo1.png",
    "platforms":[
      "android"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/src/assets/adaptive-icon.png",
        "backgroundColor": "#FF6E83"
      },
      "package": "com.nsinteriordev.idomobile",
      "versionCode": 1,
      "permissions":[
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "VIBRATE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./app/src/assets/logo1.png"
    },
    "description": ""
  }
}
